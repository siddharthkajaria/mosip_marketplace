<!-- Content Header (Page header) -->
<div class="content-header">
  <div class="container-fluid">
    <div class="row mb-2">
      <div class="col-sm-6">
        <h1 class="m-0"><%= @product_specification_version.name %> Details</h1>
      </div>
      <div class="col-sm-6">
        <ol class="breadcrumb float-sm-right">
          <li class="breadcrumb-item"><a href="<%= admin_dashboard_path %>">Dashboard</a></li>
          <li class="breadcrumb-item active">Product Specification Versions</li>
        </ol>
      </div>
    </div>
  </div>
</div>

<!-- Main content -->
<section class="content">
  <div class="container-fluid">
    <div class="row">
      <div class="col-12">
        <div class="mb-4">
          <a href="<%= admin_product_specification_versions_path %>" class="btn btn-primary">Back to Product Specification Versions</a>
          <a href="<%= edit_admin_product_specification_version_path(@product_specification_version) %>" class="btn btn-primary mr-3">
            <i class="fa fa-edit">&nbsp;</i>Edit Product Specification Version
          </a>
        </div>

        <div class="card card-primary">
          <div class="card-body">
            <dl class="row">
              <dt class="col-sm-4">Name</dt>
              <dd class="col-sm-8"><%= @product_specification_version.name %></dd>

              <dt class="col-sm-4">Description</dt>
              <dd class="col-sm-8"><%= @product_specification_version.desc %></dd>

              <dt class="col-sm-4">Code</dt>
              <dd class="col-sm-8"><%= @product_specification_version.code %></dd>

              <dt class="col-sm-4">Status</dt>
              <dd class="col-sm-8"><%= @product_specification_version.status ? 'Active' : 'Inactive' %></dd>
            </dl>
          </div>
       
